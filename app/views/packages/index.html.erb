<p id="notice"><%= notice %></p>

<h1>Listing Packages</h1>

<table>
  <thead>
    <tr>
      <th>Package dimentions</th>
      <th>Package weight</th>
      <th>Safeguard time</th>
      <th>Outbound packet time</th>
      <th>Entry packet time</th>
      <th>Batch number</th>
      <th>Client</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @packages.each do |package| %>
      <tr>
        <td><%= package.package_dimentions %></td>
        <td><%= package.package_weight %></td>
        <td><%= package.safeguard_time %></td>
        <td><%= package.outbound_packet_time %></td>
        <td><%= package.entry_packet_time %></td>
        <td><%= package.batch_number %></td>
        <td><%= package.client %></td>
        <td><%= link_to 'Show', package %></td>
        <td><%= link_to 'Edit', edit_package_path(package) %></td>
        <td><%= link_to 'Destroy', package, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Package', new_package_path %>
